install.packages("Hmisc")
library("Hmisc")
GiniMd<- function(x, na.rm=FALSE) {
if(na.rm) {
k <- is.na(x)
if(any(k)) x <- x[! k]
}
n <-length(x)
if(n < 2) return(NA)
w <- 4 * ((1 : n) - (n - 1) / 2) / n / (n - 1)
sum(w * sort(x - mean(x)))  ## center for numerical stability only
}
